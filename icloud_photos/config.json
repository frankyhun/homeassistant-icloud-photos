{
  "name": "iCloud Photos Downloader",
  "version": "1.0.7",
  "slug": "icloud_photos",
  "description": "Download photos from iCloud to Home Assistant's media folder",
  "arch": ["aarch64", "amd64", "armv7"],
  "url": "https://github.com/frankyhun/homeassistant-icloud-photos",
  "startup": "services",
  "boot": "auto",
  "options": {
    "apple_id": "",
    "password": "",
    "mfa_code": "",
    "download_path": "/config/www/icloud",
    "mode": "oneshot",
    "watch_interval": 3600,
    "auto_delete": false
  },
  "schema": {
    "apple_id": "str",
    "password": "str",
    "mfa_code": "str?",
    "download_path": "str",
    "mode": "list(oneshot|watch)",
    "watch_interval": "int",
    "auto_delete": "bool"
  },
  "map": ["media:rw", "share:rw", "config:rw"],
  "icon": "icon.png",
  "logo": "logo.png"
}